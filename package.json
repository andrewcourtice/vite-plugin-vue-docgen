{
    "name": "vite-plugin-vue-docgen",
    "version": "0.1.1",
    "description": "A simple vue-docgen plugin for Vite",
    "author": "Andrew Courtice",
    "license": "ISC",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "homepage": "https://github.com/andrewcourtice/vite-plugin-vue-docgen#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrewcourtice/vite-plugin-vue-docgen.git"
    },
    "bugs": {
        "url": "https://github.com/andrewcourtice/vite-plugin-vue-docgen/issues"
    },
    "keywords": [
        "vite",
        "plugin",
        "vue-docgen"
    ],
    "files": [
        "dist"
    ],
    "exports": {
        "import": "./dist/index.js",
        "node": "./dist/index.cjs",
        "default": "./dist/index.cjs",
        "require": "./dist/index.cjs"
    },
    "scripts": {
        "build": "tsup ./src/index.ts",
        "test": "jest"
    },
    "dependencies": {
        "@rollup/pluginutils": "^4.2.1",
        "vue-docgen-api": "^4.40.0"
    },
    "peerDependencies": {
        "vite": "^2.3.8"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "jest": "^27.0.6",
        "jest-junit": "^12.2.0",
        "ts-jest": "^27.0.3",
        "tsup": "^4.12.0",
        "typescript": "^4.3.4",
        "vite": "^2.3.8"
    },
    "jest-junit": {
        "suiteName": "Unit Tests",
        "outputDirectory": ".",
        "outputName": "./test-results.xml"
    }
}

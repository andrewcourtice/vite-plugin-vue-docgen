{
    "packageManager": "yarn@3.3.0",
    "name": "vite-plugin-vue-docgen",
    "version": "0.2.0",
    "description": "A simple vue-docgen plugin for Vite",
    "author": "Andrew Courtice",
    "license": "ISC",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "homepage": "https://github.com/andrewcourtice/vite-plugin-vue-docgen#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrewcourtice/vite-plugin-vue-docgen.git"
    },
    "bugs": {
        "url": "https://github.com/andrewcourtice/vite-plugin-vue-docgen/issues"
    },
    "keywords": [
        "vite",
        "plugin",
        "vue-docgen"
    ],
    "files": [
        "dist"
    ],
    "exports": {
        "import": "./dist/index.js",
        "node": "./dist/index.cjs",
        "default": "./dist/index.cjs",
        "require": "./dist/index.cjs"
    },
    "scripts": {
        "build": "tsup ./src/index.ts",
        "test": "jest",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "@rollup/pluginutils": "^4.2.1",
        "vue-docgen-api": "^4.52.0"
    },
    "peerDependencies": {
        "vite": "^2.3.8"
    },
    "devDependencies": {
        "@types/jest": "^29.0.1",
        "jest": "^29.0.3",
        "jest-junit": "^14.0.1",
        "ts-jest": "^29.0.0",
        "tsup": "^6.2.3",
        "typescript": "^4.8.3",
        "vite": "^3.1.0",
        "vue": "^3.2.39"
    },
    "jest-junit": {
        "suiteName": "Unit Tests",
        "outputDirectory": ".",
        "outputName": "./test-results.xml"
    }
}

{
    "packageManager": "yarn@3.3.0",
    "name": "vite-plugin-vue-docgen",
    "version": "0.3.0",
    "description": "A simple vue-docgen plugin for Vite",
    "author": "Andrew Courtice",
    "license": "ISC",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "homepage": "https://github.com/andrewcourtice/vite-plugin-vue-docgen#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrewcourtice/vite-plugin-vue-docgen.git"
    },
    "bugs": {
        "url": "https://github.com/andrewcourtice/vite-plugin-vue-docgen/issues"
    },
    "keywords": [
        "vite",
        "plugin",
        "vue-docgen"
    ],
    "files": [
        "dist"
    ],
    "exports": {
        "import": "./dist/index.js",
        "node": "./dist/index.cjs",
        "default": "./dist/index.cjs",
        "require": "./dist/index.cjs"
    },
    "scripts": {
        "build": "tsup ./src/index.ts",
        "test": "vitest",
        "prepublishOnly": "yarn build",
        "lint": "yarn run eslint . --ignore-path .gitignore --ext .ts,.mjs --quiet"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.0.2",
        "vue-docgen-api": "^4.56.0"
    },
    "peerDependencies": {
        "vite": "^2.3.8"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.29.0",
        "tsup": "^6.5.0",
        "typescript": "^4.9.3",
        "vite": "^3.2.4",
        "vitest": "^0.25.3",
        "vue": "^3.2.45"
    }
}
